{
  "name": "aktivitetsplan-felles",
  "version": "0.0.1",
  "description": "React komponenter for elementer i modig-frontend",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://stash.devillo.no/scm/fo/aktivitetsplan-felles.git"
  },
  "keywords": [
    "react",
    "modig-frontend",
    "design"
  ],
  "author": "",
  "publishConfig": {
    "registry": "http://maven.adeo.no/nexus/content/repositories/npm-internal/"
  },
  "scripts": {
    "prepublish": "in-publish && npm run build || not-in-publish",
    "lint": "esw src/",
    "clean": "rm -rf dist/ && mkdir dist",
    "build:js": "babel src/ --out-dir dist",
    "build:less": "copyfiles -f src/*.less dist",
    "build": "npm run clean && npm run build:less && npm run build:js",
    "start": "webpack-dev-server --config webpack-config.js --hot --inline --port 8080",
    "test": "lessc src/fluid-columns.less > compiled.less"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "nav-react-datepicker": "0.0.29"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "^6.18.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chokidar": "git+https://github.com/paulmillr/chokidar.git#1.5.0",
    "copyfiles": "^1.0.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "gulp": "3.9.1",
    "in-publish": "2.0.0",
    "less": "^2.7.1",
    "modig-frontend": "git+http://stash.devillo.no/scm/felles/modig-frontend.git#modig-frontend-3.0.52",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "2.5.2",
    "watchify": "3.7.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
