{
  "name": "aktivitetsplan",
  "version": "2.0.0",
  "description": "Aktivitetsplan",
  "license": "MIT",
  "scripts": {
    "start": "cross-env REACT_APP_MOCK=true craco start",
    "build:tekster": "node ./scripts/build-tekster.js",
    "build": "npm run build:tekster && craco build",
    "test": "craco test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "node integrationtest/local-runner.js",
    "test:browsers": "node integrationtest/saucelabs-runner.js",
    "lint": "npm run lint:js && npm run lint:less",
    "lint:js": "eslint src --ext .js,.jsx --fix --quiet",
    "lint:less": "lesshint --reporter ./scripts/lesshint-reporter src/",
    "prettier": "prettier --write --single-quote --trailing-comma es5 --tab-width 4 src/**/*.js src/**/*.less",
    "heroku-postbuild": "npm run build:tekster && cross-env REACT_APP_MOCK=true craco build",
    "heroku-serve": "serve -s build -l $PORT"
  },
  "dependencies": {
    "@nutgaard/use-formstate": "^2.1.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "2.2.5",
    "history": "4.6.3",
