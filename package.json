    "react-scripts": "3.1.1",
    "rebound": "^0.1.0",
    "redux": "^4.0.4",
    "redux-freeze": "^0.1.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.less": [
      "lesshint --reporter ./scripts/lesshint-reporter",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "5.4.0",
    "craco-less": "1.12.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "lesshint": "^6.3.7",
    "lint-staged": "^9.2.5",
    "prettier": "^1.5.2",
    "serve": "^11.1.0",
    "testcafe": "^1.4.2",
    "testcafe-browser-provider-saucelabs": "^1.7.0",
    "ua-parser-js": "^0.7.20",
    "yet-another-fetch-mock": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/navikt/aktivitetsplan.git"
  }
}
