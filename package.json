    "nav-frontend-lukknapp": "1.0.10",
    "nav-frontend-lukknapp-style": "0.2.11",
    "nav-frontend-modal": "1.0.11",
    "nav-frontend-modal-style": "0.3.16",
    "nav-frontend-paneler": "1.0.8",
    "nav-frontend-paneler-style": "0.3.11",
    "nav-frontend-skjema": "1.0.24",
    "nav-frontend-skjema-style": "1.0.9",
    "nav-frontend-snakkeboble": "1.0.14",
