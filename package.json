    "nav-frontend-spinner": "1.0.10",
    "nav-frontend-spinner-style": "0.2.4",
    "nav-frontend-tekstomrade": "2.0.12",
    "nav-frontend-typografi": "2.0.12",
    "nav-frontend-typografi-style": "1.0.13",
    "prop-types": "15.5.10",
