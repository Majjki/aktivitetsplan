    "nav-frontend-core": "4.0.9",
    "nav-frontend-ekspanderbartpanel": "1.0.13",
    "nav-frontend-ekspanderbartpanel-style": "0.3.13",
    "nav-frontend-etiketter": "1.0.23",
    "nav-frontend-etiketter-style": "0.3.17",
    "nav-frontend-hjelpetekst": "1.0.19",
    "nav-frontend-hjelpetekst-style": "0.3.18",
    "nav-frontend-ikoner-assets": "0.2.27",
    "nav-frontend-js-utils": "1.0.6",
    "nav-frontend-knapper": "1.0.21",
    "nav-frontend-knapper-style": "0.3.19",
    "nav-frontend-lenkepanel": "1.0.10",
