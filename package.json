    "lodash.throttle": "4.1.1",
    "moment": "2.22.1",
    "moment-duration-format": "1.3.0",
    "moment-timezone": "0.5.14",
    "nav-frontend-alertstriper": "1.0.18",
    "nav-frontend-alertstriper-style": "0.2.39",
